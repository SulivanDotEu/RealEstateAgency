        <div class="content">
            <div class="container-fluid">

                <!-- here you can write your content for the main area -->
                {% block body %}
                {% endblock body %}

            </div>
        </div>


        <footer class="footer">
            <div class="container-fluid">
                <nav class="pull-left">
                    <ul>
                        <li>
                            <a href="#">
                                Home
                            </a>
                        </li>

                        <!-- here you can add more links for the footer -->
                    </ul>
                </nav>
                <p class="copyright pull-right">
                    &copy; {{ "now"|date("Y") }} <a href="http://www.sport-finder.com">Tenerife</a>
                </p>
            </div>
        </footer>

