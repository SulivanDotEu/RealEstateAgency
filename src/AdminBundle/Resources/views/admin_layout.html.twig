    <link href="{{ asset("creativetim/css/pe-icon-7-stroke.css") }}" rel="stylesheet" />

</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-color="orange" data-image="{{ asset("creativetim/img/full-screen-image-3.jpg") }}">
        <!--

            Tip 1: you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple"
            Tip 2: you can also add an image using data-image tag

        -->

        <div class="logo">
            <a href="http://www.creative-tim.com" class="logo-text">
                Tenerife Immo
            </a>
        </div>

        <div class="sidebar-wrapper">

            <div class="user">
                {#<div class="photo">#}
                {#<img src="{{ asset("creativetim/img/default-avatar.png") }}" />#}
                {#</div>#}
                <div class="info">
                    <a data-toggle="collapse" href="#collapseExample" class="collapsed">
                        Benjamin Ellis
                        <b class="caret"></b>
                    </a>
                    <div class="collapse" id="collapseExample">
                        <ul class="nav">
                            <li><a href="#">My Profile</a></li>
                            <li><a href="#">Edit Profile</a></li>
                            <li><a href="#">Settings</a></li>
                        </ul>
                    </div>
                </div>
            </div>

            {% include '@Admin/components/left_menu.html.twig' %}
